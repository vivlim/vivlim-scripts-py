{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: picker",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test_picker.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: all unit tests",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": ["discover"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: picker ut",
            "type": "debugpy",
            "request": "launch",
            "module": "interactive.picker.test_picker",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: queue exec",
            "type": "debugpy",
            "request": "launch",
            "module": "test_queue_executor",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: async",
            "type": "debugpy",
            "request": "launch",
            "module": "test_async",
            "console": "integratedTerminal"
        },

    ]
}